{
  "logger": {
    "path": "/app-log/",
    "filename": "access.log",
    "level": "debug",
    "rotation": "20 days",
    "retention": "1 months",
    "format": "<level>{level: <8}</level> <green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> request id: {extra[request_id]} - <cyan>{name}</cyan>:<cyan>{function}:{line}</cyan> - <level>{message}</level>",
    "encoding": "utf-8"
  }
}
